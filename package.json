{
  "name": "turbo-octo-guacamole",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint",
    "postinstall": "node ./scripts/postinstall.js",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "serve": "npm run build && vite preview",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COV-GIS/turbo-octo-guacamole.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "ansi-regex": ">=6.1.0",
    "chalk": "^5.3.0",
    "eslint": "^9.10.0",
    "fs-extra": "^11.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.79.1",
    "tslib": "^2.7.0",
    "typescript": "5.5.4",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.6"
  },
  "dependencies": {
    "@arcgis/core": "^4.30.9",
    "@esri/calcite-components": "^2.12.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
